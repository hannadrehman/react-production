{
  "name": "back-end",
  "version": "1.0.0",
  "description": "a simple static web server",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=development&& export NODE_ENV_PORT=3000&& npm run start:lint",
    "start:lint": "nodemon ./src/index.js --exec 'npm run lint && node' ",
    "start:no-lint": "nodemon ./src/index.js",
    "build": "npm run test && node build.js",
    "test:watch": "npm run test -- --watchAll",
    "test": "jest",
    "lint": "esw ./src/"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "nodejs",
    "express",
    "front end server"
  ],
  "author": "hannad rehman",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "helmet": "^3.12.2",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-watch": "^4.0.2",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.2",
    "nodemon": "^1.18.3"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js?$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
